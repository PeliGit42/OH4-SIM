// Gruppen
Group P01_err_txio "Raumautomation - Platz 01" <pxc3> (err_txio)
Group P01_seg1 "Segment 1" <segment> (P01_err_txio)
Group P01_seg2 "Segment 2" <segment> (P01_err_txio)
Group P01_seg3 "Segment 3" <segment> (P01_err_txio)
Group P01_seg4 "Segment 4" <segment> (P01_err_txio)

//IOn für ERR
// Raumtempn
Group P01_err_RT "Raumtempn" <temp> (P01_err_txio)
Number	P01_NG_rs1_RT	"Raumtemperatur Segment 1 [%.1f °C]"	<temp> (P01_err_RT, P01_seg1)	{ channel="co7io-bacnet:ip-device:BAC4:SIMP01_01OHNG:NG_RS1_RT" }
Number	P01_NG_rs2_RT	"Raumtemperatur Segment 2 [%.1f °C]"	<temp> (P01_err_RT, P01_seg2)	{ channel="co7io-bacnet:ip-device:BAC4:SIMP01_01OHNG:NG_RS2_RT" }
Number	P01_NG_rs3_RT	"Raumtemperatur Segment 3 [%.1f °C]"	<temp> (P01_err_RT, P01_seg3)	{ channel="co7io-bacnet:ip-device:BAC4:SIMP01_01OHNG:NG_RS3_RT" }
Number	P01_NG_rs4_RT	"Raumtemperatur Segment 4 [%.1f °C]"	<temp> (P01_err_RT, P01_seg4)	{ channel="co7io-bacnet:ip-device:BAC4:SIMP01_01OHNG:NG_RS4_RT" }

// Lichtbefehl
Group P01_err_LGT "Lichtbefehl" <light> (P01_err_txio)
