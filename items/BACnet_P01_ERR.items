// Gruppen
Group P01_err_txio "TX-IOs PX NGA - Platz 01" <pxc> (err_txio)	
Group P01_SEG1 "Segment 1" <segment> (P01_err_txio)
Group P01_SEG2 "Segment 2" <segment> (P01_err_txio)
Group P01_SEG3 "Segment 3" <segment> (P01_err_txio)
Group P01_SEG4 "Segment 4" <segment> (P01_err_txio)

//IOn für ERR
// Raumtempn
Group P01_ERR_RT "Raumtempn" <temp> (P01_err_txio)
Number	PO1_NG_RS1_RT	"Raumtemperatur Segment 1 [%.1f °C]"	<temp> (P01_ERR_RT, P01_SEG1)	{ channel="co7io-bacnet:ip-device:BAC4:SIMP01_01OHNG:NG_RS1_RT" }
Number	PO1_NG_RS2_RT	"Raumtemperatur Segment 2 [%.1f °C]"	<temp> (P01_ERR_RT, P01_SEG2)	{ channel="co7io-bacnet:ip-device:BAC4:SIMP01_01OHNG:NG_RS2_RT" }
